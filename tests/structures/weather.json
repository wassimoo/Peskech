{
    "input": {
        "url":"https://raw.githubusercontent.com/wassimoo/Peskech/master/tests/data/NYWeather.csv",
        "format" : "CSV",
        "encoding" : "UTF-16",
        "async" : true
    },

    "structure": {
        "EST, DewPoint": {
            "$alias": "objectname",
            "$file": "path/to/class/file.py",
            "$class": "if_not_defined_filename_is_the_default_value",
            "$load": "load_to_object()",
            "$extract": "extract_from_object()",
            "$args": {
                "whole_object_alias": "$obj",
                "specific_attr_alias": "$obj.attr",
                "arg_alias_N": "$accesor"
            }
        },
        "Humidity": 0.0,
        "Sea Level": {
            "$type": "float",
            "$default": 30,
            "$options": {
                "$obj.Humidity<0": 0,
                "<30": "__ignore",
                "$obj.Visibility": "__toInt()"
            }
        },
        "{Navigation}": {
            "CloudCover": 0,
            "WindSpeedMPH": "__avg()",
            "WindDirDegree": "__min()"
        }
    },

    "output": [
        {
            "file": "path/to/output/file.json",
            "encoding": "UTF-8",
            "async": true
        },
        {
            "database": {
                "type": "ORACLE",
                "host": "",
                "port": "",
                "charset": ""
            },
            "encoding": "UTF-32",
            "async": false
        }
    ]
}